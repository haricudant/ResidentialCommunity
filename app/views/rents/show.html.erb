    <li class="bar success" style="height:<%= @rent.grossrent/21 %>%;" title="80">
      <div class="percent"> <%= @rent.grossrent.to_i %></div>
      <div class="description">Gross rent  </div>
    </li>
    
    <li class="bar secondary" style="height:<%= @rent.mortage_interest/16 %>%;" title="80">
      <div class="percent">  <%= @rent.mortage_interest.to_i %></div>
      <div class="description">MORTAGE INEREST</div>
    </li>
    
    <li class="bar primary" style="height:<%= @duck[1]/16  %>%;" title="80">
      <div class="percent"> <%= @duck[1].to_i %></div>
      <div class="description">Allowed MI</div>
    </li>
    
    <li class="bar secondary" style="height: <%= ( @duck[0])/16 %>%;" title="40">
      <div class="percent"><%=  @duck[0].to_i %> </div>
      <div class="description">Total expenses</div>
    </li>
    <li class="bar warning" style="height: <%= ( @duck[2])/18 %>%;" title="40">
      <div class="percent"><%= @duck[2].to_i %></div>
      <div class="description">Rental Profit/Loss</div>
    
    
        <li class="bar alert" style="height: <%= ( @duck[3])/18 %>%;" title="40">
      <div class="percent"><%= @duck[3].to_i %></div>
      <div class="description">Net Profit/Loss</div>
    </li>
    <li class="bar secondary" style="height:   <%= (@duck[4])/15 %>%;" title="40">
      <div class="percent"><%= @duck[4].to_i %></div>
      <div class="description">Income Tax</div>
    </li>
      <li class="bar alert" style="height: <%= ( @duck[5])/9.5 %>%;" title="40">
      <div class="percent"><%= @duck[5].to_i %></div>
      <div class="description">PRSI</div>
    </li>
    <li class="bar primary" style="height: <%= ( @duck[6])/8 %>%;" title="40">
      <div class="percent"><%= @duck[6].to_i %></div>
      <div class="description">USC</div>
    </li>
    <li class="bar success" style="height: <%= ( @duck[7])/10 %>%;" title="40">
  
      <div class="percent"><%=( @duck[7].to_i)/ 0.5 %></div>
      <div class="description">TOTAL</div>
    </li>
  </ul>
    
